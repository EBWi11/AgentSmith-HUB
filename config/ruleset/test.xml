<root name="test2" type="DETECTION">
    <rule id="reverse_shell_01" name="测试">
        <filter field="data_type">_$data_type</filter>
        <checklist condition="a and c and d and e">
            <node id="a" type="REGEX" field="exe">testcases</node>
            <node id="c" type="INCL" field="exe" logic="OR" delimiter="|">optddd|ltp</node>
            <node id="d" type="EQU" field="sessionid">_$sessionid</node>
        </checklist>
        <append field="abc">123dddd</append>
        <del>exe,argv</del>
    </rule>

    <rule id="test" name="test">
        <filter field="age">30</filter>
    </rule>

    <rule id="abc" name="abc">
        <filter field="data_type">59</filter>
        <checklist>
            <node type="EQU" field="EXE">a</node>
         </checklist>
    </rule>

    <rule id="c2_connect_02" name="测试">
        <filter field="data_type">59</filter>
        <checklist>
            <node type="REGEX" field="exe">testcases</node>
            <node type="INCL" field="argv">6400</node>
        </checklist>
    </rule>

    <rule id="reverse_shell_03" name="测试">
        <filter field="data_type">59</filter>
        <checklist>
            <node type="REGEX" field="exe">testcases</node>
            <node type="INCL" field="argv">8400</node>
        </checklist>
    </rule>

    <rule id="reverse_shell_04" name="测试">
        <filter field="data_type">59</filter>
        <checklist>
            <node type="REGEX" field="exe">will</node>
        </checklist>
    </rule>

    <rule id="hack_detection" name="检测hack程序">
        <filter field="data_type">59</filter>
        <checklist>
            <node type="INCL" field="exe">hack</node>
        </checklist>
    </rule>

    <rule id="reverse_shell_bash" name="检测bash反弹shell">
        <filter field="data_type">59</filter>
        <checklist>
            <node type="INCL" field="argv">bash -i</node>
        </checklist>
    </rule>
    <rule id="detect_msf" name="Detect malicious MSF processes when exe field indicates potential security intrusion">
        <filter field="type">process</filter>
        <checklist>
            <node type="EQU" field="exe">msf</node>
        </checklist>
    </rule>
</root>