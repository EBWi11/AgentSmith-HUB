<root ruleset_id="test1" ruleset_name="test1" type="DETECTION">
    <rule id="reverse_shell_01" name="测试" author="test">
        <filter field="data_type">_$data_type</filter>
        <checklist condition="a and (b or c)">
            <node id="a" type="REGEX" field="exe">testcases</node>
            <node id="b" type="INCL" field="argv">gfddd26</node>
            <node id="c" type="INCL" field="exe" logic="OR" delimiter="|">optddd|ltp</node>
